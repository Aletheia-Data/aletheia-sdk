  "version": "0.4.1",
  "description": "Aletheia SDK",
  "main": "AletheiaSDK.js",
  "directories": {
    "example": "examples",
    "test": "tests"
  },
  "scripts": {
    "build": "rollup --config rollup.config.js AletheiaSDK.js --file dist/bundle.js",
