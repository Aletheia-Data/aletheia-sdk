  "author": "Aletheia Data",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Aletheia-Data/aletheia-sdk/issues"
  },
  "homepage": "https://github.com/Aletheia-Data/aletheia-sdk#readme",
  "dependencies": {
    "cheerio": "^1.0.0-rc.12",
    "csvtojson": "^2.0.10",
    "jsdoc": "^4.0.2",
    "papaparse": "^5.4.1",
    "rollup": "^4.5.2",
    "taffydb": "^2.7.3",
    "xml2js": "^0.6.2"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^25.0.7",
    "better-docs": "^2.7.2",
